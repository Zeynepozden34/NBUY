@model List<Country>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ülke</th>
            <th>Başkent</th>
            <th>Nufus</th>
            <th>Dil</th>
            <th>Araç Trafiği</th>
            <th>Bayrak</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var country in Model)
        {
            <tr>
                <td>@country.Name</td>
                <td>@country.Capital</td>

            </tr>
        }
    </tbody>

</table>
